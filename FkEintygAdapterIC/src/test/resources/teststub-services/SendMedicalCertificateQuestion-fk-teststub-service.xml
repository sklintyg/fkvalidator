<mule 
	xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:cxf="http://www.mulesoft.org/schema/mule/cxf"
	xmlns:jms="http://www.mulesoft.org/schema/mule/jms"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.mulesoft.org/schema/mule/core    http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/cxf     http://www.mulesoft.org/schema/mule/cxf/current/mule-cxf.xsd
		http://www.mulesoft.org/schema/mule/jms     http://www.mulesoft.org/schema/mule/jms/current/mule-jms.xsd
	">
	
	<simple-service 
		name="SendMedicalCertificateQuestion-service-test"
        address="${inbound.endpoint.eintyg.sendmedicalcertificatequestion.teststub.test}?connector=soitoolkit-https-connector" 
        component-class="se.skl.skltpservices.adapter.fk.sendmedcertquestion.SendMedCertQuestionImpl"
        type="jax-ws" />

	<simple-service 
		name="SendMedicalCertificateQuestion-service-validate"
        address="${inbound.endpoint.eintyg.sendmedicalcertificatequestion.teststub.validate}?connector=soitoolkit-https-connector" 
        component-class="se.skl.skltpservices.adapter.fk.sendmedcertquestion.SendMedCertQuestionValidateImpl"
        type="jax-ws" />

</mule>
